" Common settings
set hls

" Key mapping
imap jj <Esc>
imap jk <Esc>A
nmap ss /
nmap SS ?
nmap B ^
nmap E $
nmap ZZ :wq<CR>
nmap QQ :q!<CR>
nmap ge :action GotoNextError<CR>
nmap gE :action GotoPreviousError<CR>
nmap gr :action Run<CR>
nmap gR :action Debug<CR>
nmap gs :action ShowErrorDescription<CR>
nmap U :action FindUsages<CR>
imap uu <up>
imap dd <down>
nmap ds di(
nmap dS di{
nmap da di'
nmap dA di"
nmap cs ci(
nmap cS ci{
nmap ca ci'
nmap cA ci"
